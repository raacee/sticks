{
  "applicationId": "com.sticksgame.dev",
  "version": "0.0.1",
  "defaultMode": "window",
  "documentRoot": "/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "sticks",
      "maxWidth": 1920,
      "maxHeight": 1080,
      "minHeight": 950,
      "minWidth": 1600,
      "height": 950,
      "width": 1600,
      "resizable": true,
      "icon": "/public/icon32.png"
    }
  },
  "cli": {
    "binaryName": "sticks",
    "resourcesPath": "/dist",
    "extensionsPath": "/extensions/",
    "binaryVersion": "4.10.0",
    "clientVersion": "3.8.2"
  }
}